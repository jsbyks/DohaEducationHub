# Backend Environment Variables for Production
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql://username:password@host:5432/database_name

# Authentication
SECRET_KEY=your-256-bit-secret-key-here-make-it-long-and-random
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS (comma-separated URLs)
CORS_ORIGINS=https://your-frontend-domain.vercel.app,https://your-custom-domain.com

# Optional: Analytics/Debug
DEBUG=False
ENVIRONMENT=production
